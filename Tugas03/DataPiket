package Tugas03;

public class DataPiket {
    private String nama;
    private String tanggal;
    private String jabatan;
    private String kehadiran;
    private String tugas;
    private String catatan;

    public DataPiket(String nama, String tanggal, String jabatan, String kehadiran, String tugas, String catatan) {
        this.nama = nama;
        this.tanggal = tanggal;
        this.jabatan = jabatan;
        this.kehadiran = kehadiran;
        this.tugas = tugas;
        this.catatan = catatan;
    }

    // Getters and setters untuk masing-masing atribut
    public String getNama() { return nama; }
    public String getTanggal() { return tanggal; }
    public String getJabatan() { return jabatan; }
    public String getKehadiran() { return kehadiran; }
    public String getTugas() { return tugas; }
    public String getCatatan() { return catatan; }
}
